<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.mlinyun.springboot3demo.dao.TbUserMapper">

    <resultMap id="BaseResultMap" type="com.mlinyun.springboot3demo.entity.TbUser">
        <id property="id" column="id"/>

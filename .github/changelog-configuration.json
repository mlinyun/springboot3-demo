{
    "categories": [
        {
            "title": "## 🚀 功能",
            "labels": [
                "feature"
            ],
            "commits": [
                "^feat"
            ]
        },
        {
            "title": "## 🐛 修复",
            "labels": [
                "fix"
            ],
            "commits": [
                "^fix"
            ]
        },
        {
            "title": "## 🧪 测试",
            "labels": [
                "test"
            ],
            "commits": [
                "^test"
            ]
        },
        {
            "title": "## 📦 构建",
            "labels": [
                "build"
            ],
            "commits": [
                "^build"
            ]
        },
        {
            "title": "## 📝 文档",
            "labels": [
                "docs"
            ],
            "commits": [
                "^docs"
            ]
        },
        {
            "title": "## 🔄 重构",
            "labels": [
                "refactor"
            ],
            "commits": [
                "^refactor"
            ]
        },
        {
            "title": "## 💄 样式",
            "labels": [
                "style"
            ],
            "commits": [
                "^style"
            ]
        },
        {
            "title": "## 🔧 配置",
            "labels": [
                "chore"
            ],
            "commits": [
                "^chore"
            ]
        },
        {
            "title": "## 其他更新",
            "labels": [
                "other"
            ],
            "exclude_labels": [
                "dependencies"
            ],
            "commits": [
                ".*"
            ]
        }
    ],
    "template": "${{CHANGELOG}}\n\n<details>\n<summary>所有提交</summary>\n\n${{COMMITS}}\n</details>",
    "pr_template": "- ${{TITLE}} (#${{NUMBER}})",
    "commit_template": "- ${{SUBJECT}}",
    "empty_template": "- 无明显更新",
    "max_commits_to_display": 20,
    "trim_values": true
}